            "name": "morethanyell"
        }
    },
    "servers": [
        {
            "url": "https://{domain}:{port}/servicesNS/-/TA-bitsight-findings",
            "variables": {
                "domain": {
                    "default": "localhost"
                },
                "port": {
                    "default": "8089"
                }
            },
            "description": "Access via management interface"
        }
    ],
    "components": {
        "schemas": {
            "logging": {
                "type": "object",
                "properties": {
                    "loglevel": {
                        "type": "string",
                        "enum": [
                            "DEBUG",
                            "INFO",
                            "WARNING",
                            "ERROR",
                            "CRITICAL"
                        ]
                    }
                }
            },
            "logging_without_name": {
                "type": "object",
